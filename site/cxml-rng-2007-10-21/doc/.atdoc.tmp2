          <simple-table>
            <tr>
              <td>
                <a href="cxml-rng__class__schema.html">
                  <tt>schema</tt>
                </a>
              </td>
            </tr>
            <tr>
              <td>
                <a href="cxml-rng__fun__parse-schema.html">
                  <tt>parse-schema</tt>
                </a>
              </td>
            </tr>
            <tr>

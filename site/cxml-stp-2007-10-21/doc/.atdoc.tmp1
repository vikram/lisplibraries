          <elt>&amp;key</elt>
          <elt>from-end</elt>
          <elt>(start 0)</elt>
          <elt>end</elt>
          <elt>key</elt>
        </lambda-list>
        <arguments>
          <arg arg="predicate">a designator for a function of one argument that returns     a generalized boolean</arg>
          <arg arg="parent">a <class id="cxml-stp__class__node">node</class></arg>
          <arg arg="from-end">a generalized boolead</arg>

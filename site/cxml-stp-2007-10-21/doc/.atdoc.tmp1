      <function-definition id="cxml-stp__fun__copy" name="copy" package="cxml-stp">
        <lambda-list>
          <elt>node</elt>
        </lambda-list>
        <arguments>
          <arg arg="node">a <class id="cxml-stp__class__node">node</class></arg>
        </arguments>
        <see-also>
          <auto>
            <see id="cxml-stp__class__node">node</see>
          </auto>
        </see-also>
        <documentation-string>    <short>This function copies a node recursively.</short><break/>
    The resulting node is of the same class as the argument, and all
    child nodes and attributes are copied in the same way.<break/>
    Shared structure includes only primitive slot values like strings.
    (The consequences are undefined if user code mutates such values, whether    <code>copy</code> is used or not.)</documentation-string>
      </function-definition>
      <function-definition id="cxml-stp__fun__map-attributes" name="map-attributes" package="cxml-stp">

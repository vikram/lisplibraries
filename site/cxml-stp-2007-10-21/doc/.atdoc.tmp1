      <function-definition id="cxml-stp__fun__rename-attribute" name="rename-attribute" package="cxml-stp">
        <lambda-list>
          <elt>attribute</elt>
          <elt>prefix</elt>
          <elt>uri</elt>
        </lambda-list>
        <arguments>
          <arg arg="attribute">the <class id="cxml-stp__class__attribute">attribute</class> to be renamed</arg>
          <arg arg="prefix">string, an NCName</arg>

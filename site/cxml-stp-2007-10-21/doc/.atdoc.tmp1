          <auto>
            <see id="cxml-stp__class__element">element</see>
            <see id="cxml-stp__class__attribute">attribute</see>
          </auto>
          <other>
            <see id="cxml-stp__fun__local-name">local-name</see>
            <see id="cxml-stp__fun__namespace-uri">namespace-uri</see>
            <see id="cxml-stp__fun__namespace-prefix">namespace-prefix</see>
          </other>
        </see-also>

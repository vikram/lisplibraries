END LC_IDENTIFICATION

LC_COLLATE
copy "iso14651_t1"

% iso14651_t1 is missing Mongolian ue(straight u), oe(barred o)
% like russian, but with <o-=> (<barred-o>) after <o=> and
% <u'=>(straight u) after <u=>

collating-symbol <o-=>
collating-symbol <u'=>

reorder-after <CYR-O>
<o-=>
reorder-after <CYR-OUBRE>
<u'=>

reorder-after <U043E>
<U04E9> <o-=>;<PCL>;<MIN>;IGNORE
reorder-after <U041E>
<U04E8> <o-=>;<PCL>;<CAP>;IGNORE

reorder-after <U0443>
<U04AF> <u'=>;<PCL>;<MIN>;IGNORE
reorder-after <U0423>
<U04AE> <u'=>;<PCL>;<CAP>;IGNORE

reorder-end
END LC_COLLATE

LC_CTYPE
copy "i18n"
END LC_CTYPE

LC_MONETARY
int_curr_symbol     "<U004D><U004E><U0054><U0020>"
currency_symbol     "<U20AE>"
mon_decimal_point   "<U002E>"
mon_thousands_sep   "<U0020>"
mon_grouping        3;3
positive_sign       ""
negative_sign       "<U002D>"
int_frac_digits     2
frac_digits         2
p_cs_precedes       0
p_sep_by_space      1
n_cs_precedes       0
n_sep_by_space      1
p_sign_posn         1
n_sign_posn         1

END LC_MONETARY

